          <!-- Metin Kısmı -->
          <div class="col-lg-12 pt-4 pt-lg-0 content">
            <h3 class="text-center mb-3">Software Developer</h3>
            
            <div class="fs-4">
              <%- about.aboutText %>
            </div>
    
