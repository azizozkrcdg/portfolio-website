    <!-- ======= Resume Section ======= -->
    <section id="resume" class="resume">
      <div class="container" data-aos="fade-up">

        <div class="section-title">
          <h2>Resume</h2>
        </div>

        <div class="row">
          <div class="col-lg-6">
            

            <h3 class="resume-title">Education</h3>
  
     
              <div class="resume-item">
                <h4>Dicle University</h4>
                <h5>Computer Programmer</h5>
                <p><em>2022 - 2024</em></p>
              </div>

              <div class="resume-item">
                <h4>Anadolu University</h4>
                <h5>Management Information Systems</h5>
                <p><em>2024 - Present</em></p>
              </div>
     
          </div>
          <div class="col-lg-6">
            <h3 class="resume-title">Professional Experience</h3>
    
            <div class="resume-item">
              <h4><%= resumes[0].company %></h4>
              <h5><%= resumes[0].jobTitle %></h5>
              <p><em><%= resumes[0].startedYear %> - Present</em></p>
              <p><strong></strong></p>
              <p> <%= resumes[0].description %></p>
            </div>
 
 

          </div>
        </div>

      </div>
    </section><!-- End Resume Section -->

