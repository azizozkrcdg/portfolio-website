      </ul>
    </nav><!-- .nav-menu -->

  </header><!-- End Header -->
  

  <!-- ======= Hero Section ======= -->
  <section id="hero" class="d-flex flex-column justify-content-center">
    <div class="container" data-aos="zoom-in" data-aos-delay="100">
      <h1>Aziz Özkaracadağ</h1>
      <p>I'm <span class="typed" data-typed-items="Software Developer!"></span></p>
      <div class="social-links">
        <% for (let i = 0; i < socialMedia.length; i++) { %>
          <% if (socialMedia[i].platform === 'linkedin') { %>
            <a href="<%= socialMedia[i].url %>" class="linkedin"><i class="bi bi-linkedin"></i></a>
          <% } else if (socialMedia[i].platform === 'github') { %>
            <a href="<%= socialMedia[i].url %>" class="github"><i class="bi bi-github"></i></a>
          <% } else if (socialMedia[i].platform === 'instagram') { %>
            <a href="<%= socialMedia[i].url %>" class="instagram"><i class="bi bi-instagram"></i></a>
          <% } else if (socialMedia[i].platform === 'twitter') { %>
            <a href="<%= socialMedia[i].url %>" class="twitter"><i class="bi bi-twitter"></i></a>
          <% } else if (socialMedia[i].platform === 'youtube') { %>
            <a href="<%= socialMedia[i].url %>" class="youtube"><i class="bi bi-youtube"></i></a>
          <% } %>
        <% } %>
      </div>
    </div>
  </section><!-- End Hero -->

